{
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Yo"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "homepage_url": "http://github.com/bih",
   "description": "Unofficial (but better) Chrome Extension for Yo. Based on original.",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png"
   },
   "manifest_version": 2,
   "name": "Better Yo",
   "permissions": [ "tabs", "geolocation", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.3"
}
